        "forceConsistentCasingInFileNames": true,
        "noImplicitReturns": true,
        "strict": true,
        "jsx": "react-jsx",
        "jsxImportSource": "preact",
        "skipLibCheck": true,
        "baseUrl": "./src/",
        "paths": {
